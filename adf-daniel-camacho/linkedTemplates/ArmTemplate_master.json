{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-daniel-camacho"
		},
		"LS_AZURESQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AZURESQL'"
		},
		"LS_DATALAKE_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_DATALAKE'"
		},
		"LS_FILESYSTEM_ONPREM_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_FILESYSTEM_ONPREM'"
		},
		"LS_AZUREVAULT_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-bigdata-train-dev.vault.azure.net/"
		},
		"LS_DATALAKE_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlscursogrm.dfs.core.windows.net/"
		},
		"LS_FILESYSTEM_ONPREM_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\\\danic\\\\OneDrive\\\\Documentos\\\\Curso_DF"
		},
		"LS_FILESYSTEM_ONPREM_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "danic"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-daniel-camacho_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AZURESQL_connectionString": {
						"value": "[parameters('LS_AZURESQL_connectionString')]"
					},
					"LS_DATALAKE_accountKey": {
						"value": "[parameters('LS_DATALAKE_accountKey')]"
					},
					"LS_FILESYSTEM_ONPREM_password": {
						"value": "[parameters('LS_FILESYSTEM_ONPREM_password')]"
					},
					"LS_AZUREVAULT_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AZUREVAULT_properties_typeProperties_baseUrl')]"
					},
					"LS_DATALAKE_properties_typeProperties_url": {
						"value": "[parameters('LS_DATALAKE_properties_typeProperties_url')]"
					},
					"LS_FILESYSTEM_ONPREM_properties_typeProperties_host": {
						"value": "[parameters('LS_FILESYSTEM_ONPREM_properties_typeProperties_host')]"
					},
					"LS_FILESYSTEM_ONPREM_properties_typeProperties_userId": {
						"value": "[parameters('LS_FILESYSTEM_ONPREM_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}